{
  "name": "lims-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:unit": "ng test --include='src/app/**/*.spec.ts' --exclude='src/app/performance-tests/**/*.spec.ts' --exclude='src/app/integration-tests/**/*.spec.ts'",
    "test:integration": "ng test --include='src/app/integration-tests/**/*.spec.ts' --browsers=ChromeHeadless --watch=false",
    "test:performance": "ng test --karma-config=src/test-config/karma.performance.conf.js --browsers=ChromeHeadlessPerformance --watch=false",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "serve:ssr:lims-app": "node dist/lims-app/server/server.mjs",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:watch": "eslint src/**/*.ts --watch",
    "lint:fix-all": "eslint src/**/*.ts --fix",
    "lint:auto-fix": "node scripts/fix-lint-issues.js",
    "prepare": "husky"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ],
    "*.tsx": [
      "eslint --fix",
      "git add"
    ],
    "*.html": [
      "eslint --fix",
      "git add"
    ]
  },
  "overrides": {
    "semver": "^7.5.4",
    "@angular/cli": {
      "semver": "^7.5.4"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.0",
    "@medplum/core": "^4.3.6",
    "@medplum/fhirtypes": "^4.3.6",
    "@medplum/react": "^4.3.6",
    "@types/qrcode": "^1.5.5",
    "express": "^5.1.0",
    "html5-qrcode": "^2.3.8",
    "qrcode": "^1.5.4",
    "react-qr-code": "^2.0.18",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.1",
    "@angular-eslint/builder": "20.1.1",
    "@angular-eslint/eslint-plugin": "20.1.1",
    "@angular-eslint/eslint-plugin-template": "20.1.1",
    "@angular-eslint/schematics": "20.1.1",
    "@angular-eslint/template-parser": "20.1.1",
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "8.57.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.2",
    "typescript": "~5.8.2"
  }
}
